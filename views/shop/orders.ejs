<%- include('../includes/head.ejs') %>
    </head>

    <body>
        <%- include('../includes/navigation.ejs') %>
        <main>
            <% if(orders.length == 0) { %>
                <h1>Nothing there!</h1>
            <% } else { %>
                <ul>
                    <% orders.forEach(e => { %>
                        <h1>#<%= e._id %></h1>
                            <ul>
                                <% e.products.forEach(p => { %>
                                    <li><%= p.product.title %> (<%= p.quantity %>)</li>
                                <% }) %>
                            </ul>
                    <% }) %>
                </ul>
               
            <% } %>
        </main>
        <%- include('../includes/end.ejs') %>